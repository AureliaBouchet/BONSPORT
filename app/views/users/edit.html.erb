<h1>Modifier mon profil</h1>

<div class="form">
  <%= simple_form_for @user do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "Modifier mon profil", class: "btn btn-default" %>
  <% end %>
</div>
